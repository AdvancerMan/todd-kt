// parameters:
// name --- level name that will be shown in "choose level" menu or level editor
// objects --- array of objects on a level

// each element of `objects` parameter has:
// type --- type of object, available types can be seen in PassiveObjectInfo class in PassiveObjectInfo.kt file
// arguments of object --- varies on object type

// example:

{
  "name": "testName",
  "objects": [
    {
      "type": "solid",
      "drawableName": "arenaSolid",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [0, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "solid",
      "drawableName": "arenaSolid",
      "bodyPattern": {
        "bodyPatternType": "rectangleWithTopGS",
        "b2dType": "static",
        "bodyPosition": [-200, -50],
        "bodySize": [200, 50]
      },
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "solid",
      "drawableName": "arenaSolid",
      "bodyPattern": {
        "bodyPatternType": "polygonWithTopGS",
        "b2dType": "static",
        "worldBodyCenter": [-300, 50],
        "localVertices": [[-100, 100], [-100, -100], [100, -100], [100, 0], [0, 100]]
      },
      "drawableSize": [200, 200],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "halfCollided",
      "drawableName": "arenaHalfCollided",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [200, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "cloudy",
      "drawableName": "arenaCloudy",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [400, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "sinceContactTillInactive": 1,
      "sinceInactiveTillActive": 1
    },

    {
      "type": "jumper",
      "drawableName": "arenaJumper",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [600, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "pushPower": 800
    },

    {
      "type": "portal",
      "drawableName": "arenaPortal",
      "bodyPatternType": "circle",
      "b2dType": "static",
      "bodyCenter": [850, -25],
      "bodyRadius": 50,
      "drawableSize": [100, 100],
      "bodyLowerLeftCornerOffset": [0, 0],
      "teleportTo": [0, 0],
      "teleportDelay": 0.1
    },

    {
      "type": "trampoline",
      "drawableName": "arenaTrampoline",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [900, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "travolator",
      "drawableName": "arenaTravolator",
      "bodyPatternType": "rectangleWithTopGS",
      "b2dType": "static",
      "bodyPosition": [1100, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "pushPower": 200
    }
  ]
}
