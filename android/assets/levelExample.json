// parameters:
// name --- level name that will be shown in "choose level" menu or level editor
// objects --- array of objects on a level

// each element of `objects` parameter has:
// type --- type of object, available types can be seen in PassiveObjectInfo class in PassiveObjectInfo.kt file
// arguments of object --- varies on object type

// example:

{
  "name": "testName",
  "objects": [
    {
      "type": "solidRectangle",
      "drawableName": "arenaSolid",
      "bodyPosition": [0, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "halfCollided",
      "drawableName": "arenaHalfCollided",
      "bodyPosition": [200, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "cloudy",
      "drawableName": "arenaCloudy",
      "bodyPosition": [400, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "sinceContactTillInactive": 1,
      "sinceInactiveTillActive": 1
    },

    {
      "type": "jumper",
      "drawableName": "arenaJumper",
      "bodyPosition": [600, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "pushPower": 800
    },

    {
      "type": "portal",
      "drawableName": "arenaPortal",
      "center": [850, -25],
      "radius": 50,
      "drawableSize": [100, 100],
      "bodyLowerLeftCornerOffset": [0, 0],
      "teleportTo": [0, 0],
      "teleportDelay": 0.1
    },

    {
      "type": "trampoline",
      "drawableName": "arenaTrampoline",
      "bodyPosition": [900, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0]
    },

    {
      "type": "travolator",
      "drawableName": "arenaTravolator",
      "bodyPosition": [1100, -50],
      "bodySize": [200, 50],
      "drawableSize": [200, 50],
      "bodyLowerLeftCornerOffset": [0, 0],
      "pushPower": 200
    }
  ]
}
